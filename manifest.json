{
  "name": "左サイドUI 拡張",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "左サイドにUIを出す",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "css": ["style.css"]
    }
  ],
  "web_accessible_resources": [{
    "resources": [
      "panels/memo/index.html"
    ],
    "matches": ["<all_urls>"]
  }],
  "permissions": []
}
